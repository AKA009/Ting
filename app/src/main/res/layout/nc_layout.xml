<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/N_IV_BG"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/bg_n_1"
        tools:layout_editor_absoluteY="411dp"/>

    <TextView
        android:id="@+id/N_TV_playerName"
        android:layout_width="120dp"
        android:layout_height="28dp"
        android:layout_marginTop="60dp"
        android:layout_toEndOf="@+id/N_B_main"
        android:text="TextView"
        android:textAlignment="viewStart"
        android:textColor="@color/my_black"
        android:textSize="16sp"/>

    <TextView
        android:id="@+id/N_TV_name"
        android:layout_width="120dp"
        android:layout_height="45dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="10dp"
        android:layout_toEndOf="@+id/N_B_main"
        android:text="TextView"
        android:textAlignment="viewStart"
        android:textColor="@color/my_black"
        android:textSize="18sp"/>

    <ImageButton
        android:id="@+id/N_B_main"
        android:layout_width="73dp"
        android:layout_height="73dp"
        android:layout_alignBottom="@+id/N_IV_BG"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="16dp"
        android:background="@android:color/transparent"
        android:scaleType="centerInside"
        app:srcCompat="@drawable/main"/>

    <ImageButton
        android:id="@+id/N_B_close"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginEnd="18dp"
        android:background="@android:color/transparent"
        android:scaleType="centerInside"
        app:srcCompat="@drawable/t_close"
        android:layout_centerVertical="true"
        android:layout_alignParentEnd="true"/>

    <ImageButton
        android:id="@+id/N_B_next"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:background="@android:color/transparent"
        android:scaleType="centerInside"
        app:srcCompat="@drawable/t_next"
        android:layout_toLeftOf="@+id/N_B_close"
        android:layout_centerVertical="true"/>

    <ImageButton
        android:id="@+id/N_B_start"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:background="@android:color/transparent"
        android:scaleType="centerInside"
        app:srcCompat="@drawable/t_start"
        android:layout_toLeftOf="@+id/N_B_next"
        android:layout_centerVertical="true"/>


</RelativeLayout>

